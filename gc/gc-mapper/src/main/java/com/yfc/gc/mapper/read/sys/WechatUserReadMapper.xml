<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yfc.gc.mapper.read.sys.WechatUserReadMapper">
    <resultMap id="BaseResultMap" type="com.yfc.gc.bean.sys.WechatUser">
        <id column="ID" jdbcType="VARCHAR" property="id" /><!-- ID 小程序用户ID -->
        <result column="OPEN_ID" jdbcType="VARCHAR" property="openId" /><!-- OPEN_ID OPEN_ID -->
        <result column="IMG" jdbcType="VARCHAR" property="img" /><!-- 用户头像 用户头像 -->
        <result column="NAME" jdbcType="VARCHAR" property="name" /><!-- 用户名 用户名 -->
        <result column="SEX" jdbcType="CHAR" property="sex" /><!-- 性别 0：女  1：男 -->
        <result column="PHONE" jdbcType="VARCHAR" property="phone" /><!-- 手机号 手机号 -->
        <result column="LONGITUDE_LATITUDE" jdbcType="VARCHAR" property="longitudeLatitude" /><!-- 经纬度 经纬度 -->
        <result column="INTEGRAL" jdbcType="INTEGER" property="integral" /><!-- 积分 积分 -->
        <result column="CREATED_BY" jdbcType="VARCHAR" property="createdBy" /><!-- 创建人 创建人 -->
        <result column="CREATED_TIME" jdbcType="TIMESTAMP" property="createdTime" /><!-- 创建时间 创建时间 -->
        <result column="UPDATED_BY" jdbcType="VARCHAR" property="updatedBy" /><!-- 更新人 更新人 -->
        <result column="UPDATED_TIME" jdbcType="TIMESTAMP" property="updatedTime" /><!-- 更新时间 更新时间 -->
    </resultMap>

</mapper>
