<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yfc.gc.mapper.read.sys.UserInfoReadMapper">
    <resultMap id="BaseResultMap" type="com.yfc.gc.bean.sys.UserInfo">
        <id column="ID" jdbcType="VARCHAR" property="id" /><!-- 用户ID 用户ID -->
        <result column="USERNAME" jdbcType="VARCHAR" property="username" /><!-- 账号 账号 -->
        <result column="PASSWORD" jdbcType="VARCHAR" property="password" /><!-- 密码 密码 -->
        <result column="NAME" jdbcType="VARCHAR" property="name" /><!-- 用户名 用户名 -->
        <result column="PHONE" jdbcType="VARCHAR" property="phone" /><!-- 电话 电话 -->
        <result column="CREATED_BY" jdbcType="VARCHAR" property="createdBy" /><!-- 创建人 创建人 -->
        <result column="CREATED_TIME" jdbcType="TIMESTAMP" property="createdTime" /><!-- 创建时间 创建时间 -->
        <result column="UPDATED_BY" jdbcType="VARCHAR" property="updatedBy" /><!-- 更新人 更新人 -->
        <result column="UPDATED_TIME" jdbcType="TIMESTAMP" property="updatedTime" /><!-- 更新时间 更新时间 -->
    </resultMap>

</mapper>
